{
  "name": "tap-junit",
  "version": "1.0.1",
  "description": "XML TAP output formatter.",
  "main": "index.js",
  "bin": {
    "tap-junit": "bin/tap-junit"
  },
  "scripts": {
    "test": "tape test/pass.js | bin/tap-junit --output output/test",
    "test:fail": "tape test/fail.js | bin/tap-junit --output output/test"
  },
  "repository": {
    "type": "git",
    "url": "https://dhershman@bitbucket.org/dhershman/tap-junit.git"
  },
  "keywords": [
    "test",
    "tap",
    "tape",
    "xml",
    "reporter",
    "xunit",
    "junit"
  ],
  "author": "Dustin Hershman",
  "license": "MIT",
  "bugs": {
    "url": "https://bitbucket.org/dhershman/tap-junit/issues"
  },
  "homepage": "https://bitbucket.org/dhershman/tap-junit/overview",
  "dependencies": {
    "duplexer": "^0.1.1",
    "fs-extra": "^2.1.2",
    "minimist": "^1.2.0",
    "tap-parser": "^0.4.3",
    "tape": "^2.14.1",
    "through2": "^0.4.1",
    "xmlbuilder": "^8.2.2"
  },
  "devDependencies": {
    "eslint": "^3.17.1"
  }
}
